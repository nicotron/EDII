void visimax(float x1, float y1, float visimaxlisb, float visimaxmad, float visimaxpari, float visimaxrom, float visimaxberl, float visimaxbern, float visimaxvien, float visimaxamst, float visimaxbrus, float visimaxlond) {

  color ver;
  ver = color(175, 253, 70); 
  
  strokeWeight(1);
  stroke(ver, visimaxlisb*8);
  line(x1, y1 -120, x1, y1 + 15 -120);

  strokeWeight(1);
  stroke(ver, visimaxmad*8);
  line(x1, y1 -45, x1, y1 + 15 - 45);

  strokeWeight(1);
  stroke(ver, visimaxpari*8);
  line(x1, y1 + 30, x1, y1 + 15 +30);

  strokeWeight(1);
  stroke(ver, visimaxrom*8);
  line(x1, y1 + 105, x1, y1 + 15 +105);

  strokeWeight(1);
  stroke(ver, visimaxberl*8);
  line(x1, y1 +180, x1, y1 + 15 +180);

  strokeWeight(1);
  stroke(ver, visimaxbern*8);
  line(x1, y1+255, x1, y1 + 15 + 255);

  strokeWeight(1);
  stroke(ver, visimaxvien*8);
  line(x1, y1+330, x1, y1 + 15 +330);

  strokeWeight(1);
  stroke(ver, visimaxamst*8);
  line(x1, y1+405, x1, y1 + 15 +405);

  strokeWeight(1);
  stroke(ver, visimaxbrus*8);
  line(x1, y1+480, x1, y1 + 15+480);

  strokeWeight(1);
  stroke(ver, visimaxlond*8);
  line(x1, y1+555, x1, y1 + 15+555);
}